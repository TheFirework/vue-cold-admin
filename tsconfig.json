{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "skipLibCheck": true,
    "types": [
      "node",
      "vite/client",
      "naive-ui/volar",
      "unplugin-vue-define-options"
    ],
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "build/**/*.ts",
    "build/**/*.d.ts",
    "types/**.ts",
    "types/**.d.ts",
    "mock/**/*.ts",
    "mock/**/*.d.ts",
    "vite.config.ts",
    "unocss.config.ts",
    "mockProdServer.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}